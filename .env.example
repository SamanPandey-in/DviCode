# React App Environment Variables
# Copy this file to .env.local and update the values

# API Configuration
REACT_APP_API_URL=http://localhost:3001/api
REACT_APP_WEBSOCKET_URL=ws://localhost:3001

# App Configuration
REACT_APP_APP_NAME=NAMASTE-ICD11 Integration
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Authentication
REACT_APP_JWT_EXPIRY=24h

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_WEBSOCKETS=true
REACT_APP_ENABLE_OFFLINE_MODE=false

# External Services (Optional)
REACT_APP_GA_TRACKING_ID=
REACT_APP_SENTRY_DSN=
REACT_APP_HOTJAR_ID=

# API Keys (Optional - for future integrations)
REACT_APP_ABDM_CLIENT_ID=
REACT_APP_FHIR_SERVER_URL=http://localhost:3001/api/fhir

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info

# UI Configuration
REACT_APP_THEME_PRIMARY_COLOR=#3b82f6
REACT_APP_THEME_SECONDARY_COLOR=#8b5cf6
REACT_APP_MAX_SEARCH_RESULTS=50
REACT_APP_SEARCH_DEBOUNCE_MS=300

# Cache Settings
REACT_APP_CACHE_TTL=300000
REACT_APP_ENABLE_SERVICE_WORKER=false

# Build Configuration
GENERATE_SOURCEMAP=true
REACT_APP_BUILD_DATE=
REACT_APP_GIT_SHA=
